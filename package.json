{
  "name": "miniplug",
  "description": "Small Promise-based plug.dj API client for Node.js.",
  "version": "1.14.0",
  "author": "RenÃ©e Kooi <renee@kooi.me>",
  "bugs": "https://github.com/goto-bus-stop/miniplug/issues",
  "contributors": [
    "Burkes (https://github.com/burkes)",
    "Cosmetify (https://github.com/cosmetify)"
  ],
  "dependencies": {
    "bluebird": "^3.5.1",
    "create-error-class": "^3.0.2",
    "debug": "^3.0.0",
    "linear-promise-backoff-queue": "^1.0.0",
    "node-fetch": "^2.1.2",
    "plug-login": "^1.2.0",
    "plug-message-split": "^1.0.0",
    "plug-socket": "^0.6.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.3",
    "buble": "^0.19.0",
    "builtin-modules": "^2.0.0",
    "glob": "^7.1.2",
    "lint-staged": "^6.1.1",
    "nock": "^9.0.27",
    "npm-run-all": "^4.0.2",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.0.1",
    "rollup": "^0.58.0",
    "rollup-plugin-buble": "^0.19.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "standard": "^11.0.0",
    "tap-min": "^1.2.1",
    "tape": "^4.6.3"
  },
  "engines": {
    "node": ">= 4"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.es.js",
    "index.es.js.map",
    "docs",
    "src"
  ],
  "greenkeeper": {
    "ignore": [
      "lint-staged"
    ]
  },
  "homepage": "https://github.com/goto-bus-stop/miniplug",
  "jsnext:main": "index.es.js",
  "keywords": [
    "plug",
    "plug.dj"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "main": "index.js",
  "module": "index.es.js",
  "pre-commit": {
    "run": [
      "rebuild",
      "tap:min",
      "lint-staged"
    ]
  },
  "repository": "goto-bus-stop/miniplug",
  "scripts": {
    "build": "rollup -c",
    "lint": "standard rollup.config.js src/**/*.js test/**/*.js",
    "lint-staged": "lint-staged",
    "prepare": "npm run build",
    "rebuild": "node has-changed || rollup -c",
    "tap": "tape -r ./test/_init.js test/*.js",
    "tap:min": "npm run tap | tap-min",
    "test": "npm-run-all lint rebuild tap:min",
    "test:ci": "npm-run-all lint rebuild tap"
  },
  "standard": {
    "env": "mocha"
  }
}
